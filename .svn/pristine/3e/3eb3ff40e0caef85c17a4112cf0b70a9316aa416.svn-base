/*layout*/

.form_congtainer {
	width: 80%;
	margin: auto;
	padding: 15px;
	overflow: hidden;
}
.bgwhite {
	background-color: #fff;
}
.bgorange {
	background-color: #f75e53;
	color: #fff;
}
.bgblue {
	background: #00aff0;
}
.ul-radio {
	padding: 5px 0px;
	display: inline-block;
	vertical-align: middle;
}
.ul-radio li {
