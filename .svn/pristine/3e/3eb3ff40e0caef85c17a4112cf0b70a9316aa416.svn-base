html {
	-webkit-text-size-adjust: 100%
}
a,
b,
body,
button,
dd,
div,
dl,
dt,
fieldset,
form,
h1,
h2,
h3,
h4,
h5,
h6,
html,
i,
iframe,
img,
input,
li,
object,
ol,
p,
pre,
span,
strong,
table,
td,
textarea,
th,
ul,
head,
footer {
	margin: 0;
	outline: 0;
	padding: 0;
	box-sizing: border-box;
}
ul,
li {
	list-style: none;
}
body {
	margin: 0;
	text-align: left;
	colorï¼š#333;
	font-family: 'microsoft yahei', Arial, sans-serif;
}
[hidden],
template {
	display: none
}
a:active,
