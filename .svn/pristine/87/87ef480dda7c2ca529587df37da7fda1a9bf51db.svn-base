    <script type="text/javascript">


        $(function () {

            $(".sideMenu").slide({
                titCell: "h3",
                targetCell: "ul",
                defaultIndex: 0,
                effect: 'slideDown',
                delayTime: '500',
                trigger: 'click',
                triggerTime: '150',
                defaultPlay: true,
                returnDefault: false,
                easing: 'easeInQuint',
                endFun: function () {
                    scrollWW();
                }
            });
            $(window).resize(function () {
                scrollWW();

            });
        });
        function scrollWW() {
            if ($(".side").height() < $(".sideMenu").height()) {
                $(".scroll").show();
                var pos = $(".sideMenu ul:visible").position().top - 38;
                $('.sideMenu').animate({top: -pos});
            } else {
                $(".scroll").hide();
                $('.sideMenu').animate({top: 0});
                n = 1;
            }
        }

        var n = 1;
        function menuScroll(num) {
            var Scroll = $('.sideMenu');
            var ScrollP = $('.sideMenu').position();
            /*alert(n);
             alert(ScrollP.top);*/
            if (num == 1) {
                Scroll.animate({top: ScrollP.top - 38});
                n = n + 1;
            } else {
                if (ScrollP.top > -38 && ScrollP.top != 0) {
                    ScrollP.top = -38;
                }
                if (ScrollP.top < 0) {
                    Scroll.animate({top: 38 + ScrollP.top});
                } else {
                    n = 1;
                }
                if (n > 1) {
                    n = n - 1;
                }
            }
        }
    </script>
    <script type="text/javascript">
        window.onload = function () {
            GetSession();
        };

        function GetSession() {
            $.ajax(
                    {
                        type: 'POST',
                        url: getLocationUrl() + "/admin/adminUserLogin/GetSession.json",
                        dataType: 'json',
                        async: async,
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            if (XMLHttpRequest.status == 500) {
                                alert('服务器内部错误.code:500.');
                            } else if (XMLHttpRequest.status == 200) {
                                alert('服务器内部错误.code:200.');
                            }
                        },
                        success: function (data) {
                            if (data.err == -1) {
                                window.parent.location.href = "login.html";
                            } else {
                                var table = new StringBuffer();
                                table.append(data.userName);
                                var tableHTML = table.toString();
                                document.getElementById("infocenter").innerHTML = tableHTML;

                                if (data.accessFlag == 0) {
                                    //无权限用户 客服？
                                    $("#jf").hide();
                                    $("#jl").hide();
                                    $("#jd").hide();
                                    $(".ZPReward").hide();
                                    $("#at").hide();
                                    $("#test").hide();
                                    $(".Ceair").hide();
                                    $(".BBG").hide();
                                    $(".West").hide();
                                    $("#QRCode").hide();
                                    
                                } else if (data.accessFlag == 2) {
                                    //东航管理员
                                    $("#jf").hide()
                                    $("#jd").hide();
                                    $(".NotCeair").hide()
                                    $("#ts").hide()
                                    $("#yj").hide()
                                    $("#at").hide()
                                    $("#test").hide();
                                    $(".BBG").hide();
                                    $(".West").hide();
                                    $("#QRCode").hide();
                                }else if(data.accessFlag==3){
                                    //ATM管理员
                                    $(".NotCeair").hide();
                                    $("#jf").hide();
                                    $("#test").hide();
                                    $("#jd").hide();
                                    $("#ts").hide();
                                    $("#yj").hide();
                                    $("#dh1").hide();
                                    $("#dh2").hide();
                                    $("#ddcc").show();
                                    $(".West").hide();
                                    $("#QRCode").hide();
                                } else if (data.accessFlag == 4) {
                                    //北部湾管理员
                                    $("#jf").hide()
                                    $("#jd").hide();
                                    $(".NotCeair").hide();
                                    $("#ts").hide();
                                    $("#yj").hide();
                                    $("#at").hide();
                                    $("#test").hide();
                                    $(".Ceair").hide()
                                    $(".West").hide();
                                    $("#QRCode").hide();
                                }else if (data.accessFlag == 5) {
                                	//航联客服经理
                                    $("#jf").hide()
                                    $("#jd").hide();
                                    $("#ts").hide();
                                    $("#yj").hide();
                                    $("#at").hide();
                                    $("#test").hide();
                                    $("#QRCode").hide();
                                } else if (data.accessFlag == 6) {
                                	//西部航空管理员
                                    $("#jf").hide()
                                    $("#jd").hide();
                                    $(".NotCeair").hide();
                                    $("#ts").hide();
                                    $("#yj").hide();
                                    $("#at").hide();
                                    $("#test").hide();
                                    $(".Ceair").hide()
                                    $(".BBG").hide();
                                } else {
                                    //超级管理员
                                    $(".Ceair").hide()
                                    $(".BBG").hide();
                                    $(".West").hide();
                                    $("#QRCode").hide();
                                }
                            }
                        },
                    });
        }


        $(function () {
